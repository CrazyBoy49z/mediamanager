<tr class="file">
    <td>
        <input type="checkbox" name="file" value="[[+id]]" />
    </td>
    <td>
        [[+name]]<br />
        <a href="[[+path]]" target="_blank">[[+path]]</a>
    </td>
    <td>
        [[+file_type]]
    </td>
    <td>
        [[+file_size]]
    </td>
    <td>
        [[+uploaded_by]]
    </td>
    <td>
        [[+upload_date]]
    </td>
</tr>